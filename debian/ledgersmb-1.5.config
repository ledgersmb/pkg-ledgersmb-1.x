#!/bin/sh

set -e

. /usr/share/debconf/confmodule

# If this is a new install, then get DB Admin user and password.
# Or also if called with 'reconfigure' instead of 'configure'?
if [ -z "$2" ];then
    db_input high ledgersmb-1.5/admin_login || true
    db_input high ledgersmb-1.5/admin_password || true
    db_go || true
fi

exit 0
